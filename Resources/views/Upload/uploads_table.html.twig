{% trans_default_domain 'upload_data' %}
{% set is_show = is_show is defined ? is_show: false %}
<table class="table table-bordered">
    <thead>
        {{ include(config.getTemplate('upload_table_header'), {config: config, is_show: is_show}) }}
    </thead>
    <tbody>
        {{ include(config.getTemplate('upload_table_rows'), {config: config,  items: items, is_show: is_show}) }}
    </tbody>
</table>